<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.innocruts.koparagaon_project.WebViewActivity">

    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webView"></WebView>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/headimage"
        android:visibility="gone"
        android:src="@drawable/cricketmatlogo"
        android:background="#fff"></ImageView>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/headimage1"
        android:visibility="gone"
        android:src="@drawable/raillogo"
        android:background="#fff"></ImageView>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/headimage2"
        android:visibility="gone"
        android:src="@drawable/livenews"
        android:background="#fff"></ImageView>

</RelativeLayout>
